{
    "family": "pist6",
    "networkMode": "awsvpc",
    "taskRoleArn": "arn:aws:iam::572098559983:role/ecsTaskRole",
    "executionRoleArn": "arn:aws:iam::572098559983:role/ecsTaskExecutionRole",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "2048",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "containerDefinitions": [
        {
            "name": "nginx",
            "image": "<IMAGE_NGINX>",
            "portMappings": [
                {
                    "name": "nginx-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "entryPoint": [],
            "command": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/pist6",
                    "awslogs-region": "ap-northeast-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        },
        {
            "name": "php",
            "image": "IMAGE_PHP",
            "portMappings": [
                {
                    "name": "php-9000-tcp",
                    "containerPort": 9000,
                    "hostPort": 9000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "DB_CONNECTION",
                    "value": "mysql"
                },
                {
                    "name": "DB_HOST",
                    "value": "pist6-stg.cluster-clrfmzriowtg.ap-northeast-1.rds.amazonaws.com"
                },
                {
                    "name": "DB_PORT",
                    "value": "3306"
                },
                {
                    "name": "DB_DATABASE",
                    "value": "pist6"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "DB_USERNAME",
                    "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:572098559983:secret:rds!cluster-c23e617a-3d11-46fb-9fc3-f0bd9dd4d32a-FKavUv"
                },
                {
                    "name": "DB_PASSWORD",
                    "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:572098559983:secret:rds!cluster-c23e617a-3d11-46fb-9fc3-f0bd9dd4d32a-FKavUv"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/pist6",
                    "awslogs-region": "ap-northeast-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}